num1 = int(input("Enter the first number: "))
num2 = int(input("Enter the second number: "))

if num1 > 8:
  print(f"{num1} is greater than 5")
elif num1 < 8:
  print(f"{num1} is less than 5")
else:
  print(f"{num1} is equal to 5")

if num2 > 8:
  print(f"{num2} is greater than 5")
elif num2 < 8:
  print(f"{num2} is less than 5")
else:
  print(f"{num2} is equal to 5")
